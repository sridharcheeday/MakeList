<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>PROJ_TYPES}"
		class="sapUiPopupWithPadding"
		items="{worklistView>/ProjTypeF4Set}"
		search="_handleProjTypeValueHelpSearch"
		confirm="_handleProjTypeValueHelpClose"
		cancel="_handleProjTypeValueHelpClose">
		<StandardListItem
			title="{worklistView>ProjTypeText}"
			description="{worklistView>ProjType}" />
	</SelectDialog>
</core:FragmentDefinition>