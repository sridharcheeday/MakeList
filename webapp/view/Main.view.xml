<mvc:View controllerName="ps.dewa.makelist.MakeList.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Page title="{i18n>appTitle}" showNavButton="true" navButtonPress="onBack">
		<content>
			<HBox justifyContent="Center" height="100%">
				<VBox width="50%" justifyContent="Center">
					<Button text="{i18n>BTN_CREATE_ML}" press="goToCreateView" type="Emphasized" width="100%" enabled="{viewModel>/bCreateButtonEnabled}"/>
					<Button text="{i18n>BTN_DISP_ML}" press="goToDisplayView" type="Emphasized" width="100%"/>
					<Button text="{i18n>BTN_CREATE_MS}" press="goToCreateMatSelView" type="Emphasized" width="100%"
						enabled="{viewModel>/bMatSelCreateButtonEnabled}"/>
					<Button text="{i18n>BTN_DISP_MS}" press="goToDisplayMatSelView" type="Emphasized" width="100%"/>
				</VBox>
			</HBox>
		</content>
	</Page>
</mvc:View>